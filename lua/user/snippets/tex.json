{
  "Beamer frame": {
    "prefix": "fr",
    "body": [
      "\\begin{frame}",
      "\t\\frametitle{$0}",
      "\\end{frame}"
    ]
  },
  "equation*": {
    "prefix": "eq*",
    "body": [
      "\\begin{equation*}",
      "\t$0",
      "\\end{equation*}"
    ]
  },
  "begin/end": {
    "prefix": "b",
    "body": [
      "\\begin{$1}",
      "\t$0",
      "\\end{$1}"
    ],
    "description": "begin/end block"
  }
}
